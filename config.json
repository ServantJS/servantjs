{
  "ip": "0.0.0.0",
  "port": 8010,
  "heartbeat": 0,

  "db": {
    "url": "mongodb://127.0.0.1:27017/servant"
  },

  "middlewares": [
    "check-workers"
  ],

  "modules": [
    {
      "name": "security",
      "enabled": true,
      "accessKey": "123"
    },
    {
      "name": "haproxy",
      "enabled": true,
      "depends": {
        "middlewares": ["check-workers"],
        "modules": ["security"]
      }
    },
    {
      "name": "monitoring",
      "enabled": false,
      "provider": "slack",
      "depends": {
        "middlewares": ["check-workers"],
        "modules": ["security"]
      }
    },
    {
      "name": "nginx",
      "enabled": true,
      "depends": {
        "middlewares": ["check-workers"],
        "modules": ["security"]
      }
    }
  ]
}
